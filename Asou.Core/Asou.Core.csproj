<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>net8.0</TargetFramework>
    <ImplicitUsings>enable</ImplicitUsings>
    <Nullable>enable</Nullable>
    <IsPackable>true</IsPackable>
    <PackageId>Asou.Core</PackageId>
    <VersionPrefix>0.1.0</VersionPrefix>
    <Authors>yamaoto</Authors>
    <Company>Asou</Company>
    <PackageDescription>ASOU is acronym for Another yet Service Orchestration Utility.</PackageDescription>
    <RepositoryUrl>https://github.com/yamaoto/Asou</RepositoryUrl>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Microsoft.Extensions.DependencyInjection.Abstractions" Version="8.0.0"/>
    <PackageReference Include="Microsoft.Extensions.Logging.Abstractions" Version="8.0.0"/>
  </ItemGroup>

  <ItemGroup>
    <ProjectReference Include="..\Asou.Abstractions\Asou.Abstractions.csproj"/>
    <ProjectReference Include="..\Asou.ContextCall\Asou.ContextCall.csproj"/>
  </ItemGroup>

